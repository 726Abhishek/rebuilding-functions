class solver(object):
	def __init__(self, _num1, _num2):
		self.num1 = num1
		self.num2 = num2

	def sum(self):
		return self.num1 + self.num2

	def divide(self):
		return self.num1 / self.num2

	def minus(self):
		return self.num1 - self.num2

	def product(self):
		return self.num1 * self.num3

class Cal(solver):
        def complex(self):
                return self.num1 * self.num2 + self.num1 - self.num2 /self.num2

val = int (input ("Enter the num1 : "))
val1 = int (input ("Enter the num2 : "))
p1 = Cal(val, val1)
result = p1.complex()
print(result)
